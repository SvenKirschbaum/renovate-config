{
  "extends": [
    "config:base",
    "group:recommended",
    "group:monorepos"
  ],
  "timezone": "Europe/Berlin",
  "rebaseWhen": "conflicted",
  "lockFileMaintenance": {
    "enabled": true,
    "groupName": "all non-major dependencies",
    "groupSlug": "all-minor-patch",
    "schedule": "before 4am on sunday",
    "commitMessageAction": "Update",
    "addLabels": [
      "maintenance"
    ]
  },
  "transitiveRemediation": true,
  "vulnerabilityAlerts": {
    "addLabels": ["security"]
  },
  "hostRules": [
    {
      "matchHost": "docker.io",
      "hostType": "docker",
      "username": "fallobst22",
      "encrypted": {
        "password": ""
      }
    }
  ],
  "major": {
    "addLabels": [
      "major"
    ]
  },
  "minor": {
    "addLabels": [
      "minor"
    ]
  },
  "patch": {
    "addLabels": [
      "patch"
    ]
  },
  "digest": {
    "addLabels": [
      "digest"
    ]
  },
  "packageRules": [
    {
      "matchPackagePatterns": [
        "*"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch",
        "digest"
      ],
      "groupName": "all non-major dependencies",
      "groupSlug": "all-minor-patch",
      "schedule": "before 4am on sunday"
    },
    {
      "groupName": "oidc-client-ts",
      "matchPackageNames": [
        "oidc-client-ts",
        "react-oidc-context"
      ]
    },
    {
      "matchPackagePatterns": ["^@aws-cdk\/.*-alpha$"],
      "ignoreUnstable": false
    }
  ]
}
