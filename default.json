{
  "extends": [
    "config:base"
  ],
  "rebaseWhen": "conflicted",
  "lockFileMaintenance": {
    "enabled": true
  },
  "transitiveRemediation": true,
  "vulnerabilityAlerts": {
    "addLabels": ["security"]
  },
  "major": {
    "addLabels": [
      "major"
    ]
  },
  "minor": {
    "addLabels": [
      "minor"
    ]
  },
  "patch": {
    "addLabels": [
      "patch"
    ]
  },
  "packageRules": [
    {
      "matchPackageNames": ["aws-sdk", "constructs"],
      "schedule": ["after 9pm on sunday"]
    }
  ]
}
